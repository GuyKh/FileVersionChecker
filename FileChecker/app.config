<?xml version="1.0"?>
<configuration>
  <appSettings>
    
    <!-- Prefixes for filenames for example - Company.Product.Version.dll-->
    <add key="Prefixes" value="MyPrefix,Prefix,Pre" />
    
    <!-- Extentions to check-->
    <add key="Extentions" value=".exe,.dll" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
